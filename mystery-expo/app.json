{
  "expo": {
    "name": "mystery-expo",
    "slug": "mystery-expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mystery-expo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mysterymontero.mysteryexpo"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.mysterymontero.mysteryexpo"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2d6fdaff-8467-4787-a3d1-b3ee7b3ae72f"
      }
    },
    "owner": "mysterymontero",
    "updates": {
      "url": "https://u.expo.dev/2d6fdaff-8467-4787-a3d1-b3ee7b3ae72f"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
